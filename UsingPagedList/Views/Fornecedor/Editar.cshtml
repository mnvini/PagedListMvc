@model UsingPagedList.Models.Fornecedor
@{
    ViewBag.Title = "Editar";
}

<h2>Editar</h2>
@using (Html.BeginForm("Editar","Fornecedor", FormMethod.Post))
{
    @Html.HiddenFor(f => f.Id)
    @Html.HiddenFor(f => f.DataCadastro)
    <div class="col-md-12 form-group">
        <label >Nome:</label>
        @Html.TextBoxFor(f => f.Nome, new {@class = "form-control "})
        <br/>
        <label>CNPJ:</label>
        @Html.TextBoxFor(f => f.CNPJ, new {@class = "form-control"})
        <br/>
        <label>E-Mail:</label>
        @Html.TextBoxFor(f => f.Email, new {@class = "form-control"})
    </div>
    <br/>
    <input type="submit" class="btn btn-primary"/>
}
